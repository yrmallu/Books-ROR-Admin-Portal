<h1>Listing licenses</h1>

<table>
  <thead>
    <tr>
      <th>License group</th>
      <th>Datetime</th>
      <th>No of licenses</th>
      <th>Allocated to</th>
      <th>Used liscenses</th>
      <th>School</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @licenses.each do |license| %>
      <tr>
        <td><%= license.license_group_id %></td>
        <td><%= license.datetime %></td>
        <td><%= license.no_of_licenses %></td>
        <td><%= license.allocated_to %></td>
        <td><%= license.used_liscenses %></td>
        <td><%= license.school_id %></td>
        <td><%= link_to 'Show', license %></td>
        <td><%= link_to 'Edit', edit_license_path(license) %></td>
        <td><%= link_to 'Destroy', license, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New License', new_license_path %>
